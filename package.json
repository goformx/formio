{
  "name": "goforms-template",
  "version": "0.1.0",
  "description": "Form.io template library",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "npm run clean && vite build",
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "formio",
    "goforms",
    "template"
  ],
  "author": "Russell Jones <russell@web.net>",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "mocha": "^11.2.2",
    "nyc": "^17.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "@formio/js": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@formio/js": {
      "glob": "^10.3.10"
    }
  }
}
