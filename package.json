{
  "name": "goforms",
  "version": "0.1.0",
  "description": "GoForms form.io template library",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "types": "lib/mjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/mjs/index.d.ts",
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js",
      "default": "./lib/mjs/index.js",
      "build": "./src/index.ts",
      "vite": "./src/index.ts"
    },
    "./components": {
      "types": "./lib/mjs/templates/components/index.d.ts",
      "import": "./lib/mjs/templates/components/index.js",
      "require": "./lib/cjs/templates/components/index.js",
      "default": "./lib/mjs/templates/components/index.js",
      "build": "./src/templates/components/index.ts",
      "vite": "./src/templates/components/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/mjs/*.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "npm run clean && tsc --project tsconfig.cjs.json && tsc --project tsconfig.mjs.json && gulp templates && webpack",
    "watch": "gulp watch",
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "mocha --require ts-node/register --reporter spec './{,!(node_modules)/**/}*.spec.ts'",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublish": "npm run build",
    "postinstall": "git config core.hooksPath .husky"
  },
  "keywords": [
    "formio",
    "goforms",
    "template"
  ],
  "author": "Russell Jones <russell@web.net>",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/ejs": "^3.1.5",
    "@types/gulp": "^4.0.17",
    "@types/gulp-rename": "^2.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "ejs": "^3.1.10",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "gulp": "^5.0.0",
    "gulp-ejs": "^5.1.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^6.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^9.1.7",
    "mocha": "^11.2.2",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.32.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1",
    "webpack-stream": "^7.0.0"
  },
  "peerDependencies": {
    "@formio/js": "^5.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "@formio/js": {
      "glob": "^10.3.10"
    },
    "glob": "^9.3.5",
    "rimraf": "^5.0.5"
  }
}
